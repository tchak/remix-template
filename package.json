{
  "private": true,
  "name": "remix-template",
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:start": "remix dev",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:clean": "del .cache build public/build app/styles",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "lint": "run-s lint:*",
    "lint:types": "tsc",
    "lint:js": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "size": "run-s build size-limit",
    "size-limit": "size-limit",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:deploy": "yarn prisma migrate deploy",
    "start": "remix-serve ./build",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "3.12.0",
    "@reach/skip-nav": "^0.16.0",
    "@reach/tooltip": "^0.16.2",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "1.3.5",
    "@remix-run/serve": "1.3.5",
    "clsx": "^1.1.1",
    "is-hotkey": "^0.2.0",
    "isbot": "^3.4.5",
    "luxon": "^2.3.1",
    "nodemailer": "^6.7.3",
    "prisma": "^3.12.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hotkeys-hook": "^3.4.4",
    "remix": "1.3.5",
    "remix-auth": "^3.2.1",
    "remix-auth-email-link": "^1.3.1",
    "remix-utils": "^3.0.0",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.7.0",
    "use-debounce": "^7.0.1",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@remix-run/dev": "1.3.5",
    "@remix-run/eslint-config": "1.3.5",
    "@size-limit/preset-app": "^7.0.8",
    "@tailwindcss/forms": "^0.5.0",
    "@types/is-hotkey": "^0.1.7",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^4.0.0",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "size-limit": "^7.0.8",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "public/build/**/*.js"
    }
  ],
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/node",
      "@remix-run/eslint-config/jest",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-redeclare": "off"
    },
    "settings": {
      "jest": {
        "version": 27
      }
    },
    "overrides": [
      {
        "files": ["tailwind.config.js", "remix.config.js", "jest.config.js"],
        "env": {
          "node": true
        }
      }
    ]
  }
}
